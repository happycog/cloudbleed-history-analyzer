{
  "name": "CloudflareHistoryChecker",
  "version": "0.1.0",
  "main": "main.es6",
  "scripts": {
    "webpack": "node node_modules/webpack/bin/webpack.js",
    "electron": "electron . --enable-logging",
    "dev": "npm run webpack && npm run electron",
    "clean": "rm -rf ./dist",
    "css": "npm-css css/src.css > css/main.css",
    "pack": "npm run clean && electron-packager . \"Cloudflare History Checker\" --out=dist/osx --platform=darwin --arch=x64 --ignore=dist --ignore=README.md --ignore=.gitignore",
    "postinstall": "install-app-deps"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "electron": "^1.4.15",
    "electron-builder": "^14.1.1",
    "electron-packager": "^8.5.2",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "progressbar.js": "^1.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "request": "^2.79.0",
    "sqlite3": "^3.1.8",
    "underscore": "^1.8.3"
  }
}
